{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"linebreak-style": "off",
		"no-console": "off",
		"no-tabs": "off",
		"indent": ["warn", "tab"],
		"comma-dangle": "off",
		"func-names": ["warn", "never"],
		"no-underscore-dangle": "off",
		"no-param-reassign": "off",
		"no-plusplus": "off",
		"operator-linebreak": ["warn", "after"],
		"max-len": ["warn", { "code": 90 }],
		"implicit-arrow-linebreak": "off",
		"function-paren-newline": "off"
	}
}
